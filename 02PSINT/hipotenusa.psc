Algoritmo hipotenusa
	cateto1=0;
	cateto2=0;
	hipo=0; 
	resultado=0; 
	escribir "Ingresa a "; 
	leer cateto1; 
	escribir "ingresa b"; 
	leer cateto2; 
	resultado= (cateto1*cateto1)+(cateto2*cateto2);
	hipo=RC(resultado);
	escribir "el resultado es : " , hipo; 
